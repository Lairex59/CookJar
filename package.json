{
  "name": "cook-jar",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:ci": "ng test --watch=false --browsers=ChromeHeadlessCustom",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "compodoc": "./node_modules/.bin/compodoc -p tsconfig.doc.json --disablePrivate",
    "compodoc:build": "compodoc -p tsconfig.doc.json",
    "compodoc:build-and-serve": "compodoc -p tsconfig.doc.json -s",
    "compodoc:serve": "compodoc -s",
    "start-pwa": "ng build && http-server -p 8080 -c-1 dist/CookJar",
    "gh-pages": "gh-pages",
    "predeploy": "ng build --configuration production--base-href=/opendatahub-ahmed-demetz/",
    "deploy": "npx gh-pages -d dist/CookJar"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~15.2.0",
    "@angular/cdk": "~15.2.1",
    "@angular/common": "~15.2.0",
    "@angular/compiler": "~15.2.0",
    "@angular/core": "~15.2.1",
    "@angular/flex-layout": "^12.0.0-beta.35",
    "@angular/forms": "~15.2.0",
    "@angular/material": "~12.2.13",
    "@angular/platform-browser": "~15.2.0",
    "@angular/platform-browser-dynamic": "~15.2.0",
    "@angular/router": "~15.2.0",
    "@angular/service-worker": "~15.2.0",
    "@compodoc/compodoc": "^1.1.19",
    "@ericblade/quagga2": "^1.8.2",
    "@tensorflow/tfjs": "^4.2.0",
    "@types/uuid": "^9.0.0",
    "dexie": "^3.2.3",
    "file-saver": "^2.0.5",
    "flag-icons": "^6.6.6",
    "hammerjs": "^2.0.8",
    "html2canvas": "^1.4.1",
    "http-server": "^14.1.1",
    "jspdf": "^2.5.1",
    "ngx-barcode-scanner": "^14.0.1",
    "ngx-cookie-service": "^15.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.1.0",
    "turndown": "^7.1.1",
    "uuid": "^9.0.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~15.2.0",
    "@angular/cli": "^15.2.4",
    "@angular/compiler-cli": "~15.2.0",
    "@types/file-saver": "^2.0.5",
    "@types/jasmine": "~4.3.1",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.2",
    "@types/turndown": "^5.0.1",
    "angular-cli-ghpages": "^1.0.5",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "ngx-spec": "^2.0.0",
    "typescript": "^4.8.0",
    "gh-pages": "^5.0.0"
  }
}
